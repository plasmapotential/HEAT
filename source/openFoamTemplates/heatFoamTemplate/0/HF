FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      HF;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    STLpatch
    {
        type            timeVaryingMappedFixedValue;
        setAverage      0;
        offset          0.0;
        mapMethod       nearest;
    };
};
